              {% if class.anonymous %}
                <span class="text-white/40">{{ class.filePath }}</span><br>
                <a href="../classes/{{ class.id }}.html" class="font-semibold inline-block" title="{{ class.name }}">{{ class.name }}</a>
              {% else %}
                <span class="text-white/40">{{ class.filePath }}</span>
                {% if class.namespace %}<span class="text-white/40">{{ class.namespace }}</span><br>{% endif %}
                <a href="../classes/{{ class.id }}.html" class="font-semibold inline-block" title="{{ class.name }}">{{ class.singleName }}</a>
              {% endif %}
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.methods|length }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.methods|length }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ function.loc }}">
              <span class="inline-block px-1.5 py-0.5{% if class.predictionTooLong %} rounded-md bg-yellow-700{% endif %}">{{ class.loc }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ function.lloc }}">
              <span class="inline-block px-1.5 py-0.5{% if class.predictionTooLong %} rounded-md bg-yellow-700{% endif %}">{{ class.lloc }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ function.cloc }}">
              <span class="inline-block px-1.5 py-0.5{% if class.cloc == 0 %} rounded-md bg-yellow-700{% endif %}">{{ class.cloc }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.htmlPercentage }}">
              <span class="inline-block px-1.5 py-0.5{% if class.predictionTooMuchHtml == true %} rounded-md bg-yellow-700{% endif %}">{{ class.htmlPercentage|number_format(2) }}%</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ function.avgMethodCc }}">
              <span class="inline-block px-1.5 py-0.5{% if class.predictionTooComplex %} rounded-md bg-red-900{% endif %}">{{ class.avgMethodCc|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.maintainabilityIndex }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.maintainabilityIndex|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.difficulty }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.difficulty|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.effort }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.effort|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.complexityDensity }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.complexityDensity|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.lcom }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.lcom }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.instability }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.instability|number_format(2) }}</span>
            </td>
            <td class="border border-slate-500 text-right px-2 py-0.5" data-sort="{{ class.abstractness }}">
              <span class="inline-block px-1.5 py-0.5">{{ class.abstractness|number_format(2) }}</span>
            </td>
          </tr>
        {% endfor %}
        </tbody>
      </table>
    </div>
  {% endif %}
</div>

