{
  "name": "php-code-archeology/php-code-archeology",
  "description": "Static analyzer for PHP project archeology. Calculates various metrics for your codebase.",
  "keywords": [
    "analysis",
    "php",
    "testing",
    "qa",
    "quality"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PhpCodeArch\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "authors": [
    {
      "name": "Marcus Kober",
      "email": "hello@marcuskober.de",
      "homepage": "https://marcuskober.com",
      "role": "Copyright Holder"
    }
  ],
  "homepage": "https://PhpCodeArcheology.org",
  "support": {
    "issues": "https://github.com/PhpMetrics/PhpMetrics/issues"
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "twig/twig": "^3.10",
    "mockery/mockery": "^1.6",
    "composer/composer": "^2.7"
  },
  "config": {
    "allow-plugins": {
      "composer/composer": true,
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "nikic/php-parser": "^v5.1",
    "php": ">=8.2.0",
    "symfony/yaml": "^7.1",
    "composer-plugin-api": "*"
  },
  "bin": [
    "bin/phpcodearcheology"
  ],
  "extra": {
    "class": "PhpCodeArch\\ComposerEventSubscriber"
  }
}
